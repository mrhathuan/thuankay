<!DOCTYPE html>

<html>
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
    <!-- Meta, title, CSS, favicons, etc. -->
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>@ViewBag.Title</title>
    <!--CSS-->
    
    <!-- Bootstrap core CSS -->
    
    <link rel="stylesheet" href="http://maxcdn.bootstrapcdn.com/bootstrap/3.3.6/css/bootstrap.min.css">
    @*<link href="~/fonts/css/font-awesome.min.css" rel="stylesheet" />
    <link href="~/Content/Admin/css/animate.min.css" rel="stylesheet" />*@
    <!-- CSS Custom styling plus plugins -->
    @*<link href="~/Content/Admin/css/custom.css" rel="stylesheet" />
    <link href="~/Content/Admin/css/datatables/css/jquery.dataTables.min.css" rel="stylesheet" />
    <link href="~/Content/Admin/css/datatables/css/scroller.bootstrap.min.css" rel="stylesheet" />
    <link href="~/Content/Admin/css/datatables/css/buttons.dataTables.min.css" rel="stylesheet" />*@
    @Styles.Render("~/Content/CssAdmin")
    <link rel="stylesheet" href="~/Content/kendo/2016.1.112/kendo.common.min.css" type="text/css" />
    <link rel="stylesheet" href="~/Content/kendo/2016.1.112/kendo.dataviz.min.css" type="text/css" />
    <link rel="stylesheet" href="~/Content/kendo/2016.1.112/kendo.default.min.css" type="text/css" />
    <link rel="stylesheet" href="~/Content/kendo/2016.1.112/kendo.dataviz.default.min.css" type="text/css" />
    @RenderSection("CssHead", required: false)
    <!---Script--->
    
</head>
<body class="nav-md">
        <div class="container body">
                <div class="main_container">
                    <div class="col-md-3 left_col">
                        <div class="left_col scroll-view">
                            @{ Html.RenderAction("_MenuLeft", "Pn");}
                            <!-- /menu footer buttons -->
                            <div class="sidebar-footer hidden-small">
                                <a data-toggle="tooltip" data-placement="top" title="Settings">
                                    <i class="fa fa-wrench"></i>
                                </a>
                                <a data-toggle="tooltip" data-placement="top" title="FullScreen">
                                    <i class="" aria-hidden="true"></i>
                                </a>
                                <a data-toggle="tooltip" data-placement="top" title="Lock">
                                    <span class="fa fa-remove" aria-hidden="true"></span>
                                </a>
                                <a data-toggle="tooltip" data-placement="top" title="Logout">
                                    <span class="fa fa-power-off" aria-hidden="true"></span>
                                </a>
                            </div>
                            <!-- /menu footer buttons -->
                        </div>
                    </div>
                    <!--Top-->
                    @{ Html.RenderAction("_TopAdmin", "Pn");}
                    <!--MAIN-->
                    <div class="right_col" role="main">
                        <div class="">
                            <div class="row">
                                <div class="col-md-12 col-sm-12 col-xs-12">
                                    <div class="x_panel">
                                        <!--Title-->
                                        <div class="x_title">
                                            <h2>@RenderSection("title",required:false)
                                             </h2>
                                            <ul class="nav navbar-right panel_toolbox">
                                                <li>
                                                    <a class="collapse-link"><i class="fa fa-chevron-up"></i></a>
                                                </li>
                                                <li>
                                                    <a class="close-link"><i class="fa fa-close"></i></a>
                                                </li>
                                            </ul>
                                            <div class="clearfix"></div>
                                        </div>
                                        <!--content-->
                                        <div class="x_content">
                                            @if (TempData["AlertMessage"] != null)
                                            {
                                                <div id="AlertBox" class="alert @TempData["AlertType"] hide">
                                                    @TempData["AlertMessage"]
                                                </div>
                                            }                                          
                                            @RenderBody()
                                        </div>
                                    </div>
                                
                                </div>
                            </div>
                        </div>
                        <footer>
                            <div class="pull-right">
                                Admin Template edit by <a href="https://www.facebook.com/ThuanKay">Thuận Kày</a>
                            </div>
                            <div class="clearfix"></div>
                        </footer>
                        <!-- /footer content -->
                    </div>
                    <!--///-->
                    <!-- footer content -->                   
                    </div>
                </div>
    <div id="custom_notifications" class="custom-notifications dsp_none">
        <ul class="list-unstyled notifications clearfix" data-tabbed_notifications="notif-group"></ul>
        <div class="clearfix"></div>
        <div id="notif-group" class="tabbed_notifications"></div>
    </div>
    <!---SCRIPT--> 
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/1.12.2/jquery.min.js"></script>
    @Scripts.Render("~/bundles/JsAdmin")
    
    @*<script src="~/Scripts/Admin/bootstrap.min.js"></script>
    <script src="~/Scripts/Admin/custom.js"></script>   
    <script src="~/Scripts/Admin/validator/validator.js"></script>      
    <script src="~/Scripts/Plugin/ckfinder/ckfinder.js"></script>
    <script src="~/Scripts/Plugin/ckeditor/ckeditor.js"></script>*@
    @RenderSection("JsFooter", required: false)
    <script type="text/javascript" src="~/Scripts/Admin/kendo/2016.1.112/kendo.all.min.js"></script>
    <script type="text/javascript" src="~/Scripts/Admin/kendo/2016.1.112/kendo.aspnetmvc.min.js"></script>
    <script type="text/javascript" src="~/Scripts/Admin/kendo/2016.1.112/jszip.min.js"></script>
    <!--Begin Ckeditor-->
    <script>
        var editor = CKEDITOR.replace('txtContent', {
            customConfig: '/Scripts/Plugin/ckeditor/config.js',
        });
    </script>
    <!--Alert-->
    <script type="text/javascript">
         $(function () {
             $('#AlertBox').removeClass('hide');
             $('#AlertBox').delay(3000).slideUp(500);
         });
    </script>
    <!---->
</body>
</html>
